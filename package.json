{
  "name": "smartrecipe",
  "version": "1.0.0",
  "description": "This project demonstrates a multi-database setup using SQL, MongoDB, and Neo4j for a recipe recommendation system. It includes a Node.js backend with Express.js to handle CRUD operations. Prisma is used as the ORM for SQL databases.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "nodemon app.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed:mongo": "node mongoDB/seed.js",
    "seed:neo4j": "node neo4j/neo4jSeeder.js",
    "seed:mysql": "node prisma/seed.js",
    "seed": "node prisma/seed.js && node neo4j/neo4jSeeder.js && node mongoDB/seed.js",
    "seed:all": "node mongoDB/seed.js & node neo4j/neo4jSeeder.js & node prisma/seed.js",
    "migrate": "prisma migrate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@prisma/client": "^6.3.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "mongoose": "^8.2.0",
    "mysql2": "^3.12.0",
    "neo4j-driver": "^5.27.0",
    "prisma": "^6.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "@babel/preset-env": "^7.x.x",
    "jest": "^29.x.x",
    "supertest": "^6.x.x"
  }
}
